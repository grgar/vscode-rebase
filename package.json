{
  "name": "rebase",
  "displayName": "Git Rebase Keybindings",
  "publisher": "grg",
  "repository": {
    "type": "git",
    "url": "https://github.com/grgar/vscode-rebase"
  },
  "description": "Shortcuts for Git rebase editor",
  "icon": "images/icon-light-small.png",
  "version": "0.1.2",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Keymaps"
  ],
  "author": {
    "name": "grg",
    "url": "https://grg.app",
    "email": "vscode@grg.app"
  },
  "homepage": "https://github.com/grgar/vscode-rebase",
  "qna": "https://github.com/grgar/vscode-rebase/discussions",
  "bugs": {
    "email": "vscode@grg.app",
    "url": "https://github.com/grgar/vscode-rebase/issues"
  },
  "galleryBanner": {
    "theme": "light",
    "color": "#f6f8fa"
  },
  "contributes": {
    "keybindings": [
      {
        "key": "p",
        "command": "runCommands",
        "when": "editorTextFocus && (vim.mode == 'Normal' || !vim.active) && editorLangId == 'git-rebase'",
        "args": {
          "commands": [
            {
              "command": "cursorMove",
              "args": {
                "to": "wrappedLineStart"
              }
            },
            {
              "command": "deleteWordRight"
            },
            {
              "command": "editor.action.insertSnippet",
              "args": {
                "snippet": "pick"
              }
            }
          ]
        }
      },
      {
        "key": "r",
        "command": "runCommands",
        "when": "editorTextFocus && (vim.mode == 'Normal' || !vim.active) && editorLangId == 'git-rebase'",
        "args": {
          "commands": [
            {
              "command": "cursorMove",
              "args": {
                "to": "wrappedLineStart"
              }
            },
            {
              "command": "deleteWordRight"
            },
            {
              "command": "editor.action.insertSnippet",
              "args": {
                "snippet": "reword"
              }
            }
          ]
        }
      },
      {
        "key": "e",
        "command": "runCommands",
        "when": "editorTextFocus && (vim.mode == 'Normal' || !vim.active) && editorLangId == 'git-rebase'",
        "args": {
          "commands": [
            {
              "command": "cursorMove",
              "args": {
                "to": "wrappedLineStart"
              }
            },
            {
              "command": "deleteWordRight"
            },
            {
              "command": "editor.action.insertSnippet",
              "args": {
                "snippet": "edit"
              }
            }
          ]
        }
      },
      {
        "key": "s",
        "command": "runCommands",
        "when": "editorTextFocus && (vim.mode == 'Normal' || !vim.active) && editorLangId == 'git-rebase'",
        "args": {
          "commands": [
            {
              "command": "cursorMove",
              "args": {
                "to": "wrappedLineStart"
              }
            },
            {
              "command": "deleteWordRight"
            },
            {
              "command": "editor.action.insertSnippet",
              "args": {
                "snippet": "squash"
              }
            }
          ]
        }
      },
      {
        "key": "f",
        "command": "runCommands",
        "when": "editorTextFocus && (vim.mode == 'Normal' || !vim.active) && editorLangId == 'git-rebase'",
        "args": {
          "commands": [
            {
              "command": "cursorMove",
              "args": {
                "to": "wrappedLineStart"
              }
            },
            {
              "command": "deleteWordRight"
            },
            {
              "command": "editor.action.insertSnippet",
              "args": {
                "snippet": "fixup"
              }
            }
          ]
        }
      },
      {
        "key": "b",
        "command": "runCommands",
        "when": "editorTextFocus && (vim.mode == 'Normal' || !vim.active) && editorLangId == 'git-rebase'",
        "args": {
          "commands": [
            {
              "command": "editor.action.insertLineAfter"
            },
            {
              "command": "editor.action.insertSnippet",
              "args": {
                "snippet": "break"
              }
            }
          ]
        }
      },
      {
        "key": "x",
        "command": "runCommands",
        "when": "editorTextFocus && vim.mode == 'Normal' && editorLangId == 'git-rebase'",
        "args": {
          "commands": [
            {
              "command": "editor.action.insertLineAfter"
            },
            {
              "command": "editor.action.insertSnippet",
              "args": {
                "snippet": "exec "
              }
            },
            {
              "command": "extension.vim_insert"
            }
          ]
        }
      }
    ]
  }
}
